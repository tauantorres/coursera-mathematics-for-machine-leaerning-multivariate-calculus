    "import numpy as np\n",
    "\n",
    "sigma = np.tanh\n",
    "W = np.array([[-2, 4, -1],[6, 0, -3]])\n",
    "b = np.array([0.1, -2.5])\n",
    "\n",
    "def res_a1(a0):\n",
    "    return sigma(W @ a0 + b)\n",
    "\n",
    "a0 = np.array([0.3, 0.4, 0.1])\n",
    "a1 = res_a1(a0)\n",
    "print(a1)"
   ]
